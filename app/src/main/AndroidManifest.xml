<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher_2"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_2_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.TechCru"
        tools:targetApi="31">
        <activity
            android:name=".ErrorActivity"
            android:exported="false"
            android:theme="@style/error"/>
        <activity
            android:name=".Activity3"
            android:exported="false"
            android:theme="@style/activity3" />
        <activity
            android:name=".Activity2"
            android:exported="false"
            android:theme="@style/activity2" />
        <activity
            android:name=".Activity1"
            android:exported="false"
            android:theme="@style/activity1" />
        <activity
            android:name=".Setting_Community_Legal"
            android:exported="false"
            android:theme="@style/activity1" />
        <activity
            android:name=".MainActivity"
            android:exported="true"
            android:theme="@style/main">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="com.facebook.sdk.ApplicationId"
            android:value="@string/facebook_app_id" />
        <meta-data
            android:name="com.facebook.sdk.ClientToken"
            android:value="@string/facebook_client_token" />

        <uses-library
            android:name="org.apache.http.legacy"
            android:required="false" />

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>